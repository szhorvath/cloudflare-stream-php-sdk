{
    "name": "szhorvath/cloudflare-stream-php-sdk",
    "description": "A PHP SDK for integrating Cloudflare Streaming services.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Szhorvath\\CloudflareStream\\": "src/",
            "Szhorvath\\CloudflareStream\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Sandor Horvath",
            "email": "z.sandor.horvath@gmail.com"
        }
    ],
    "scripts": {
        "test": [
            "@php vendor/bin/pest"
        ],
        "pest": "vendor/bin/pest --parallel",
        "pest-coverage": "vendor/bin/pest --coverage",
        "pest-type": "vendor/bin/pest --type-coverage",
        "pint": "vendor/bin/pint"
    },
    "require": {
        "php": "^8.1",
        "psr/http-client": "^1.0",
        "psr/http-message": "^1.0",
        "php-http/curl-client": "^2.3",
        "guzzlehttp/psr7": "^2.7",
        "php-http/message": "^1.16"
    },
    "require-dev": {
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-type-coverage": "^2.8",
        "laravel/pint": "^1.17",
        "phpstan/phpstan": "^1.11"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    }
}

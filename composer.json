{
    "name": "szhorvath/cloudflare-stream-php-sdk",
    "description": "A PHP SDK for integrating Cloudflare Streaming services.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Szhorvath\\CloudflareStream\\": "src/",
            "Szhorvath\\CloudflareStream\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Sandor Horvath",
            "email": "z.sandor.horvath@gmail.com"
        }
    ],
    "scripts": {
        "test": [
            "@php vendor/bin/pest"
        ],
        "pest": "vendor/bin/pest --parallel",
        "pest-coverage": "vendor/bin/pest --coverage",
        "pest-type": "vendor/bin/pest --type-coverage",
        "pint": "vendor/bin/pint"
    },
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-fileinfo": "*",
        "psr/http-message": "^2.0",
        "psr/http-client-implementation": "*",
        "php-http/httplug": "^2.4",
        "psr/http-factory": "^1.1",
        "php-http/discovery": "^1.19",
        "psr/http-factory-implementation": "*",
        "php-http/client-common": "^2.7",
        "php-http/cache-plugin": "^2.0",
        "illuminate/collections": "^11.19",
        "league/object-mapper": "dev-main",
        "nesbot/carbon": "^3.7"
    },
    "require-dev": {
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-type-coverage": "^2.8",
        "laravel/pint": "^1.17",
        "phpstan/phpstan": "^1.11",
        "php-http/mock-client": "^1.6",
        "php-http/message": "^1.16",
        "symfony/http-client": "^7.1",
        "nyholm/psr7": "^1.8",
        "spatie/ray": "^1.41"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": false
        }
    }
}
